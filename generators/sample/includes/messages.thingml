	message temperatureMessage(TemperatureData : UInt32 , currentHour: UInt32);
	message telemetryMessage(power : UInt32 , stateOnOff: Boolean);
}
thing fragment timerMsgs {
	message timerMessage(currentHour : UInt32);
	message getTime();
}
