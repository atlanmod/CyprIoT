<?xml version="1.0" encoding="ASCII"?>
<cyprIoT:CyprIoTModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:cyprIoT="http://www.atlanmod.org/CyprIoT">
  <importThings name="Thing1" assignedRoles="//@roles.0 //@roles.1" importPath="&quot;thing1.thingml&quot;"/>
  <importThings name="Thing2" assignedRoles="//@roles.0 //@roles.1" importPath="&quot;thing2.thingml&quot;"/>
  <pubsubs name="Broker">
    <hasTopics name="topic1"/>
    <hasTopics name="light" subtopicOf="//@pubsubs.0/@hasTopics.0"/>
  </pubsubs>
  <networks name="twoThingsCom">
    <instancesThing name="thing1" typeThing="//@importThings.0" platform="@posix"/>
    <instancesThing name="thing2" typeThing="//@importThings.1" platform="@java"/>
    <instancesPubsub name="CentralMqtt" typePubSub="//@pubsubs.0" platform="@mqtt"/>
    <bindsPubsub thingInstance="//@networks.0/@instancesThing.0" subjectPort="command" readOrWrite="&lt;=" pubSubInstance="//@networks.0/@instancesPubsub.0" topics="//@pubsubs.0/@hasTopics.0 //@pubsubs.0/@hasTopics.1"/>
    <bindsPubsub thingInstance="//@networks.0/@instancesThing.1" subjectPort="command" readOrWrite="=>" pubSubInstance="//@networks.0/@instancesPubsub.0" topics="//@pubsubs.0/@hasTopics.0"/>
  </networks>
  <roles name="sensor"/>
  <roles name="actuator"/>
  <roles name="actor"/>
</cyprIoT:CyprIoTModel>
