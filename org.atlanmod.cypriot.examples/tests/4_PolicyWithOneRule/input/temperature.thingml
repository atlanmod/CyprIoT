import "includes/messages.thingml"


thing TemperatureSensor includes tempMsgs {
	
	provided port SendingTemperaturePort {
		sends temperatureMessage
	}
	statechart temperature init low {
		state low {}
		state high {}
	}
}

configuration thing2Exec {
	instance temperatureSensor:TemperatureSensor
	connector temperatureSensor.SendingTemperaturePort over X
}
 