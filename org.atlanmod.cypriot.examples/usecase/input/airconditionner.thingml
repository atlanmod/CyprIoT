import "includes/messages.thingml"


thing AirConditioner includes tempMsgs {
	
	required port ReceivingTemperaturePort {
		receives temperatureMessage
	}
	statechart airConditionner init idle {
		state idleAC {}
		state workAC {}
	}
}

configuration AirConditionerCfg {
	instance airConditioner:AirConditioner
	connector airConditioner.ReceivingTemperaturePort over X
} 