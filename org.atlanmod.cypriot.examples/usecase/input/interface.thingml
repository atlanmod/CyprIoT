import "includes/messages.thingml"


thing Interface includes tempMsgs {
	
	required port ReadingTemperaturePort {
		sends temperatureMessage
		receives temperatureMessage
	}
	statechart Interface init idle {
		state idle {}		
		state update {}
	}
}

configuration LightSwitchCfg {
	instance interface:Interface
	connector interface.ReadingTemperaturePort over X
}