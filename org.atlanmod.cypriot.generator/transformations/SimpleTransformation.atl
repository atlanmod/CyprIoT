-- @atlcompiler emftvm
-- @path ThingML=/CyprIoT/org.atlanmod.cypriot.generator/transformations/metamodels/ThingML.ecore

module SimpleTransformation;

create OUT: ThingML from TH: ThingML;

rule rule1 {
	from 
		th : ThingML!ThingMLModel
	to
		model: ThingML!ThingMLModel (
			configs <- th.configs,
			imports <- th.imports,
			protocols <- th.protocols,
			types <- th.types
		)
}