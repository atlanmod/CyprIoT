thing HelloThing {
	message m(mess:UInt8)
	provided port myport {
		receives m
	}
	statechart HelloStateChart init HelloState {
		
		final state HelloState {
			on entry do println  "Hello World!" end
		}
	}
}

datatype UInt8<1>
    @type_checker "Integer"
	@c_type "uint8_t"
	@java_type "byte"
	@js_type "byte"