thing HelloThing {
	message m(mess:UInt8)
	provided port receiveInfo {
		receives m
	}
	statechart HelloStateChart init HelloState {
		
		state HelloState {
			internal event e : receiveInfo?m
			action println e.mess
		}
	}
}

datatype UInt8<1>
    @type_checker "Integer"
	@c_type "uint8_t"
	@java_type "byte"
	@js_type "byte"