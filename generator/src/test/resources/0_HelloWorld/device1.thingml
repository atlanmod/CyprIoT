thing device1 {
	message m(mess : String)
	required port myport @sync_send `true` {
		sends m
	}
	statechart HelloStateChart init HelloState {
		final state HelloState {
			on entry do
				println "Hello World!"
				myport!m("okokoko")
			end
		}
	}
}

datatype String<20>
	@serializable
	@c_type "char *"
	@arduino_type "String"
	@java_type "String"
    @js_type "String"
    @go_type "string";

