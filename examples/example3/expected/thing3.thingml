thing thing1{
	message message1()
	required port portConsume {
		receives message1
	}
	statechart thing1 init state1 {
		state state1 {
			transition -> state2
			event portConsume?message1 
		}
		state state2 {}
	}
}
protocol MQTT;
configuration thing1Cfg {
	instance thing1Inst:thing1
	connector thing1.portConsume over MQTT @mqtt_subscribe "org/atlanmod/cypriot/example2/topic1"
}
